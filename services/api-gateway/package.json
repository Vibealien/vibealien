{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "Central API Gateway with routing, rate limiting, and authentication",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vibecode/types": "workspace:*",
    "@vibecode/utils": "workspace:*",
    "cors": "^2.8.5",
