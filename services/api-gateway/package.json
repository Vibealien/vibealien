    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "rate-limit-redis": "^4.2.0",
    "redis": "^5.8.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
